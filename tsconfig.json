{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@gm50x/common": [
        "libs/common/src"
      ],
      "@gm50x/common/*": [
        "libs/common/src/*"
      ],
      "@gm50x/environment": [
        "libs/environment/src"
      ],
      "@gm50x/environment/*": [
        "libs/environment/src/*"
      ],
      "@gm50x/database": [
        "libs/databases/src"
      ],
      "@gm50x/database/*": [
        "libs/databases/src/*"
      ],
      "@gm50x/swagger": [
        "libs/swagger/src"
      ],
      "@gm50x/swagger/*": [
        "libs/swagger/src/*"
      ],
      "@gm50x/auth": [
        "libs/auth/src"
      ],
      "@gm50x/auth/*": [
        "libs/auth/src/*"
      ]
    }
  }
}